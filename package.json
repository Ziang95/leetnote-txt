{
    "name": "txt-note",
    "displayName": "txt_note",
    "description": "a simple *keyword highlighter* for those who want to take leetcde study note in txt format",
    "version": "0.0.1",
    "publisher": "Zian",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "txt",
            "aliases": ["txt", "txt"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "txt",
            "scopeName": "source.t",
            "path": "./syntaxes/txt.tmLanguage.json"
        }]
    }
}