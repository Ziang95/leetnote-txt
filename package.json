{
    "name": "txt_leetnote",
    "displayName": "txt_leetnote",
    "description": "a simple Keyword Highlighter for those who want to take leetcode study notes in txt format",
    "version": "0.0.3",
    "publisher": "Zian",
    "license": "MIT",
    "engines": {
        "vscode": "^1.36.0"
    },
    "repository": {
        "url": "https://github.com/zeaan/leetnote-txt"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "txt",
            "aliases": ["txt", "txt"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "txt",
            "scopeName": "source.t",
            "path": "./syntaxes/txt.tmLanguage.json"
        }]
    }
}